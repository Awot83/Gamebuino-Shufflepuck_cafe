        gb.display.drawBitmap(0, 0, BEJIN[StatAnim]);
        break;
    }


    // Frequence des modif de l'animation
    y = y+1;
   if (y < 15) {StatAnim = 0;}
   if (y > 30 && y < 45){StatAnim = 1;}
   if (y > 45 && y < 60) {StatAnim = 2;}
   if (y > 60) {y=0;}


    // controle du changement d'animation manuelle
    if (gb.buttons.pressed(BTN_A)) {
      StatAnim++;
      if(StatAnim==1){y=31;}
      if(StatAnim==2){y=46;}
      if(StatAnim==3){y=0;}
              
      if (StatAnim < 0) {
        StatAnim = 0;
      }
      if (StatAnim > 2) {
        StatAnim = 2;
      }
    }

    // Controle : changement de personnage

    if (gb.buttons.pressed(BTN_RIGHT)) {
      x++;
      y=0;  //reset animation to 1rst
      if (x > 5) {
        x = 5;
      }
    }
    if (gb.buttons.pressed(BTN_LEFT)) {
      x--;
      y=0;  //reset animation to 1rst
      if (x < 1) {
        x = 1;
      }
    }

    if (gb.buttons.pressed(BTN_B)) {   // press B pour retourner au bar
      x = 0;
     y=0;  //reset animation to 1rst
    }



  }
}





